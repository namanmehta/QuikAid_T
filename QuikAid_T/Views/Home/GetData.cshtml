@model IEnumerable<QuikAid_T.Models.MyClass>

@{
    ViewBag.Title = "GetData";
    if (Session["userID"] == null)
    {
        Response.Redirect("~/Login/Index");
    }
}


<h2>Get Data</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CountOfClients)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CountOfClients)
            </td>
            @*<td>
                
                @Html.ActionLink("Details", "Detail", new { id = item.FirstName }) 
                
            </td>*@
        </tr>
    }

</table>
